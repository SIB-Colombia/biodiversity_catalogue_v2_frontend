function _taggedTemplateLiteral(strings, raw) { return Object.freeze(Object.defineProperties(strings, { raw: { value: Object.freeze(raw) } })); }

import React from 'react';
import PropTypes from 'prop-types';
import { injectGlobal, ThemeProvider } from 'styled-components';
import theme from './themes/default';
import MuiThemeProvider from 'material-ui/styles/MuiThemeProvider';

injectGlobal(_templateObject, theme.fonts.primary, theme.palette.secondary[0], theme.fonts.xs, theme.palette.grayscale[6], theme.fonts.xs, theme.fonts.sm, theme.fonts.xs, theme.fonts.md, theme.fonts.xs, theme.fonts.lg, theme.palette.primary[0], theme.palette.grayscale[0], theme.palette.primary[0], theme.palette.primary[0], theme.palette.primary[0], theme.palette.secondary[0], theme.palette.grayscale[0], theme.palette.secondary[0], theme.palette.secondary[0], theme.palette.secondary[0], theme.palette.basescale[9], theme.palette.basescale[7], theme.palette.basescale[8], theme.palette.secondary[0], theme.palette.text[0], theme.palette.primary[0], theme.palette.secondary[0], theme.fonts.text[0]);

var App = function App(_ref) {
  var children = _ref.children;


  window.scrollTo(0, 0);

  var wallpaper = '';

  switch (children.props.route.path) {
    case '/login':
      wallpaper = '23890_orig.jpg';
      break;
    case '/static':
      wallpaper = '82168_orig.jpg';
      break;
    case '/file':
      if (children.props.location.pathname.indexOf('file/summary') > -1) {
        wallpaper = 'tucan.jpg';
      }
      break;
    default:
  }

  wallpaper = wallpaper ? '\n    background-image: url(/background/' + wallpaper + ');\n    background-size: cover;\n    background-position: center center;\n    background-attachment:fixed;\n    ' : 'background: ' + theme.palette.grayscale[1] + ';';

  injectGlobal(_templateObject2, wallpaper);

  return React.createElement(
    MuiThemeProvider,
    { theme: theme },
    children
  );
};

export default App;